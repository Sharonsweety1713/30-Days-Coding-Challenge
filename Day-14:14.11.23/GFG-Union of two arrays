class Solution{
    public static int doUnion(int a[], int n, int b[], int m) 
    {
        //Your code here
        int tot=n+m;
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<m;j++)
            {
                if(a[i]==b[j])
                tot--;
            }
        }
        return tot;
    }
}
