import java.util.* ;
import java.io.*; 
import java.util.ArrayList;

public class Solution {
    public static void divideByFour(ArrayList<Integer> arr){
        // Write your code here.
        int ans=0;
        for(int i=0;i<arr.size();i++)
        {
            ans=arr.get(i)/4;
            if(ans==0)
            arr.set(i,-1);
            else
            arr.set(i,ans);
        }
    }
}
